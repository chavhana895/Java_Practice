
public class Q8 {

	public static void main(String[] args) 
	{
	  for (int i = 0; i < 1; i++) 
	  {
		 for (int j = 0; j < 3; j++) 
		 {
		   if(j==0)
			   System.out.print((char)(72+j));
		   else if(j==1)
			   System.out.print((char)(13+65));
		   else if(j==2)
		     System.out.print((char)(25+65));
		   
		 }
	  }
	}

}
