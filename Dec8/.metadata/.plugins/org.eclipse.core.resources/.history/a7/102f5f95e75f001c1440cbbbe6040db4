	private static void LoopI(String s,String s1, int i) 
	{
	  if(i<s.length())
	  {
		  LoopJ(s,s1,i,0);
		  LoopI(s,s1,i+1);
	  }
	}

	private static void LoopJ(String s, String s1, int i, int j) 
	{
		if(j<s.length())
		{
			if(s.charAt(i)=='d')
			{
			  
			}
		}
	}

}
