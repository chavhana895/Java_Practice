package Dec18;

public class Loop {

	public static void main(String[] args) 
	{
	  String s = "avinash";
	  String s1="";
	  for (int i = 0; i < s.length(); i++) 
	  {
		for (int j = 0; j < s1.length(); j++) 
		{
			if(s.charAt(i)=='a')
				s1=s1+s.charAt(i);
				
		}
	}
	  System.out.println(s1);
	}

}
