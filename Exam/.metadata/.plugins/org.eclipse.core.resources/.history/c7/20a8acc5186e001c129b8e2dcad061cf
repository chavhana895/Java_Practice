package LinkedList;

public class MyLinkedList <p>
{
Node <p> root;

void add(p data)
{
	Node<p> obj = new Node<p>(data);
	if(root==null)
	{
		root=obj;
	}
	else
	{
		Node<p> temp=root;
		while(temp.next!=null)
		{
			temp=temp.next;
		}
		temp.next=obj;
	}
}

void print()
{
	Node<p> temp=root;
	while(temp!=null)
	{
		System.out.println(temp.data);
		temp=temp.next;
	}
}

p remove()
{
	
	
	return null;
	
}
}
